{
  "states": [
    "Init",
    "Bound",
    "Renewing",
    "Rebinding"
  ],
  "initial_state": "Init",
  "final_states": [],
  "transitions": [
    {
      "from": "Init",
      "event": "receive DHCPOFFER",
      "action": "send DHCPREQUEST",
      "to": "Bound"
    },
    {
      "from": "Bound",
      "event": "timeout T1",
      "action": "send DHCPREQUEST",
      "to": "Renewing"
    },
    {
      "from": "Renewing",
      "event": "receive DHCPACK",
      "action": "set lease renewed",
      "to": "Bound"
    },
    {
      "from": "Renewing",
      "event": "timeout T2",
      "action": "send DHCPREQUEST",
      "to": "Rebinding"
    },
    {
      "from": "Rebinding",
      "event": "receive DHCPACK",
      "action": "set lease rebound",
      "to": "Bound"
    },
    {
      "from": "Rebinding",
      "event": "timeout lease expiration",
      "action": "reset configuration",
      "to": "Init"
    }
  ]
