{
  "states": [
    "CLOSED",
    "LISTEN",
    "SYN-SENT",
    "SYN-RECEIVED",
    "ESTABLISHED",
    "FIN-WAIT-1",
    "FIN-WAIT-2",
    "CLOSE-WAIT",
    "CLOSING",
    "LAST-ACK",
    "TIME-WAIT"
  ],
  "initial_state": "CLOSED",
  "final_states": [
    "LAST-ACK",
    "TIME-WAIT"
  ],
  "transitions": [
    {
      "from": "CLOSED",
      "requisite": "OPEN call",
      "to": "LISTEN",
      "actions": [
        "create TCB"
      ],
      "response": "local connection name"
    },
    {
      "from": "LISTEN",
      "requisite": "rcv SYN",
      "to": "SYN-RECEIVED",
      "actions": [
        "set RCV.NXT",
        "send SYN,ACK"
      ],
      "response": "acknowledge SYN"
    },
    {
      "from": "SYN-SENT",
      "requisite": "rcv SYN,ACK",
      "to": "ESTABLISHED",
      "actions": [
        "set SND.UNA",
        "send ACK"
      ],
      "response": "acknowledge SYN,ACK"
    },
    {
      "from": "ESTABLISHED",
      "requisite": "CLOSE call",
      "to": "FIN-WAIT-1",
      "actions": [
        "send FIN"
      ],
      "response": "acknowledge FIN"
    },
    {
      "from": "FIN-WAIT-1",
      "requisite": "rcv FIN",
      "to": "FIN-WAIT-2",
      "actions": [
        "send ACK"
      ],
      "response": "acknowledge FIN"
    },
    {
      "from": "FIN-WAIT-2",
      "requisite": "rcv FIN",
      "to": "TIME-WAIT",
      "actions": [
        "send ACK"
      ],
      "response": "acknowledge FIN"
    },
    {
      "from": "TIME-WAIT",
      "requisite": "timeout",
      "to": "CLOSED",
      "actions": [
        "delete TCB"
      ],
      "response": "connection closed"
    },
    {
      "from": "SYN-RECEIVED",
      "requisite": "rcv RST",
      "to": "LISTEN",
      "actions": [
        "delete TCB"
      ],
      "response": "return to LISTEN"
    },
    {
      "from": "CLOSE-WAIT",
      "requisite": "CLOSE call",
      "to": "LAST-ACK",
      "actions": [
        "send FIN"
      ],
      "response": "acknowledge FIN"
    },
    {
      "from": "CLOSING",
      "requisite": "rcv ACK",
      "to": "TIME-WAIT",
      "actions": [
        "delete TCB"
      ],
      "response": "acknowledge FIN"
    }
  ]
}